{
  "name": "Link Saver",
  "version": "1.1.0",
  "description": "Save and organize links in workspaces and categories",
  "manifest_version": 3,
  "permissions": ["tabs", "storage", "activeTab"],
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "service_worker": "js/background.js"
  }
}
